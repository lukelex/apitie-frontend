<h2>
  {{entity.name}}
</h2>
<ul>
  {{#each field in entity.fields}}
    {{#unless field.internal}}
      {{field-label model=field action='openModal'}}
    {{/unless}}
  {{/each}}

  {{#if view.shouldShowMore}}
    <li class="more"><span>+{{view.extraPublicFields}} more</span></li>
  {{/if}}
</ul>
